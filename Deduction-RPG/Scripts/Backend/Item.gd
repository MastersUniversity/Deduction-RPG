class_name Item

var name = ""        # The item's name (also used as index)
var description = "" # The item's description
var image = ""       # The image associated with this item

func set_name(new_name):
	name = new_name
	
func set_description(new_description):
	description = new_description
	
func set_image(new_image):
	image = new_image
